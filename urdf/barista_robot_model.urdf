<?xml version="1.0"?>
<robot name="box_bot">

  <material name="red">
      <color rgba="1.0 0.0 0.0 1"/>
  </material>

  <material name="green">
      <color rgba="0.0 1.0 0.0 1"/>
  </material>

  <material name="blue">
      <color rgba="0.0 0.0 1.0 1"/>
  </material>

  <material name="black">
      <color rgba="0.0 0.0 0.0 1"/>
  </material>

  <material name="white">
      <color rgba="1.0 1.0 1.0 1"/>
  </material>

  <material name="box_bot_blue">
      <color rgba="0.088656 0.428691 0.491021 1"/>
  </material>

  <link name="base_link">
  </link>

  <!-- Body -->
  <link name="chassis">
    <visual>
      <geometry>
        <cylinder length="0.155" radius="0.178"/>
      </geometry>
      <material name="red"/>
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.155" radius="0.178" />
      </geometry>
    </collision>
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <mass value="0.5" />
      <inertia ixx="0.00496154167" ixy="0.0" ixz="0.0" iyy="0.00496154167" iyz="0.0" izz="0.007921" />
    </inertial>
  </link>

  <joint name="base_link_joint" type="fixed">
    <origin rpy="0 0 0" xyz="0 0 0" />
    <parent link="base_link" />
    <child link="chassis" />
  </joint>

  
  <link name="rod_1">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <cylinder length="0.22" radius="0.01"/>
      </geometry>
      <material name="white"/>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <cylinder length="0.22" radius="0.01"/>
      </geometry>
    </collision>
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <mass value="0.5" />
      <inertia ixx="0.00202916667" ixy="0.0" ixz="0.0" iyy="0.00202916667" iyz="0.0" izz="0.000025" />
    </inertial>
  </link>
  
  <joint name="rod_1_joint" type="fixed">
    <parent link="base_link"/>
    <child link="rod_1"/>
    <origin rpy="0 0 0" xyz="0.130 0.05 0.185" />
  </joint>

  
  <link name="rod_2">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <cylinder length="0.22" radius="0.01"/>
      </geometry>
      <material name="white"/>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <cylinder length="0.22" radius="0.01"/>
      </geometry>
    </collision>
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <mass value="0.5" />
      <inertia ixx="0.00202916667" ixy="0.0" ixz="0.0" iyy="0.00202916667" iyz="0.0" izz="0.000025" />
    </inertial>
  </link>
  
  <joint name="rod_2_joint" type="fixed">
    <parent link="base_link"/>
    <child link="rod_2"/>
    <origin xyz="0.130 -0.05 0.185" rpy="0 0 0"/>
  </joint>


  <link name="rod_3">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <cylinder length="0.22" radius="0.01"/>
      </geometry>
      <material name="white"/>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <cylinder length="0.22" radius="0.01"/>
      </geometry>
    </collision>
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <mass value="0.5" />
      <inertia ixx="0.00202916667" ixy="0.0" ixz="0.0" iyy="0.00202916667" iyz="0.0" izz="0.000025" />
    </inertial>
  </link>
  
  <joint name="rod_3_joint" type="fixed">
    <parent link="base_link"/>
    <child link="rod_3"/>
    <origin xyz="-0.130 0.05 0.185" rpy="0 0 0"/>
  </joint>

  
  <link name="rod_4">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <cylinder length="0.22" radius="0.01"/>
      </geometry>
      <material name="white"/>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <cylinder length="0.22" radius="0.01"/>
      </geometry>
    </collision>
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <mass value="0.5" />
      <inertia ixx="0.00202916667" ixy="0.0" ixz="0.0" iyy="0.00202916667" iyz="0.0" izz="0.000025" />
    </inertial>
  </link>
  
  <joint name="rod_4_joint" type="fixed">
    <parent link="base_link"/>
    <child link="rod_4"/>
    <origin xyz="-0.130 -0.05 0.185" rpy="0 0 0"/>
  </joint>

<link name="cupholder_link">
  <visual>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <geometry>
      <cylinder length="0.09" radius="0.16"/>
    </geometry>
  </visual>
  <collision>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <geometry>
      <cylinder length="0.09" radius="0.16"/>
    </geometry>
  </collision>
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <mass value="1" />
      <inertia ixx="0.007075" ixy="0.0" ixz="0.0" iyy="0.007075" iyz="0.0" izz="0.0128" />
    </inertial>
</link>

<joint name="cupholder_joint" type="fixed">
  <parent link="base_link"/>
  <child link="cupholder_link"/>
  <origin xyz="0 0 0.330" rpy="0 0 0"/>
</joint>

  <link name="left_wheel">
    <visual>
      <origin rpy="0 1.5707 1.5707" xyz="0 0 0" />
      <geometry>
        <cylinder length="0.0206" radius="0.0352" />
      </geometry>
      <material name="black" />
    </visual>

    <collision>
      <origin rpy="0 1.5707 1.5707" xyz="0 0 0" />
      <geometry>
        <cylinder length="0.0206" radius="0.0352" />
      </geometry>
    </collision>

    <inertial>
      <origin rpy="0 1.5707 1.5707" xyz="0 0 0"/>
      <mass value="0.5" />
      <inertia ixx="0.000172561667" ixy="0.0" ixz="0.0" iyy="0.000172561667" iyz="0.0" izz="0.00030976" />
    </inertial>
  </link>


  <joint name="joint_left_wheel" type="continuous">
    <origin rpy="0 0 0" xyz="0 0.15 -0.0775" />
    <child link="left_wheel" />
    <parent link="chassis" />
    <axis rpy="0 0 0" xyz="0 1 0" />
    <limit effort="1000" velocity="100" />
    <joint_properties damping="1.0" friction="1.0" />
  </joint>

  <link name="right_wheel">
    <visual>
      <origin rpy="0 1.5707 1.5707" xyz="0 0 0" />
      <geometry>
        <cylinder length="0.0206" radius="0.0352" />
      </geometry>
      <material name="black" />
    </visual>

    <collision>
      <origin rpy="0 1.5707 1.5707" xyz="0 0 0" />
      <geometry>
        <cylinder length="0.0206" radius="0.0352" />
      </geometry>
    </collision>

    <inertial>
      <origin rpy="0 1.5707 1.5707" xyz="0 0 0"/>
      <mass value="0.5" />
      <inertia ixx="0.000172561667" ixy="0.0" ixz="0.0" iyy="0.000172561667" iyz="0.0" izz="0.00030976" />
    </inertial>
  </link>

  <joint name="joint_right_wheel" type="continuous">
    <origin rpy="0 0 0" xyz="0 -0.15 -0.0775" />
    <child link="right_wheel" />
    <parent link="chassis" />
    <axis rpy="0 0 0" xyz="0 1 0" />
    <limit effort="1000" velocity="100" />
    <joint_properties damping="1.0" friction="1.0" />
  </joint>




  <!-- Caster Wheel Front -->
  <link name="front_yaw_link">
    <visual>
        <origin rpy="0 1.5707 1.5707" xyz="0 0 0"/>
        <geometry>
          <cylinder length="0.015" radius="0.025"/>
        </geometry>
        <material name="blue"/>
      </visual>
    <collision>
      <origin rpy="0 1.5707 1.5707" xyz="0 0 0" />
      <geometry>
            <cylinder length="0.015" radius="0.025"/>
      </geometry>
    </collision>
  </link>

  <joint name="front_yaw_joint" type="continuous">
    <origin rpy="0 0 0" xyz="0.10 0 -0.0770" />
    <parent link="chassis" />
    <child link="front_yaw_link" />
    <axis xyz="0 0 1" />
    <limit effort="1000.0" velocity="100.0" />
    <dynamics damping="0.0" friction="0.1"/>
  </joint>

  <link name="front_roll_link">
      <visual>
        <origin rpy="0 1.5707 1.5707" xyz="0 0 0"/>
        <geometry>
          <cylinder length="0.015" radius="0.025"/>
        </geometry>
        <material name="red"/>
      </visual>
      <collision>
      <origin rpy="0 1.5707 1.5707" xyz="0 0 0" />
      <geometry>
        <cylinder length="0.015" radius="0.025"/>
      </geometry>
    </collision>

  </link>

  <joint name="front_roll_joint" type="continuous">
    <origin rpy="0 0 0" xyz="0 0 0" />
    <parent link="front_yaw_link" />
    <child link="front_roll_link" />
    <axis xyz="1 0 0" />
    <limit effort="1000.0" velocity="100.0" />
    <dynamics damping="0.0" friction="0.1"/>
  </joint>

  <link name="front_pitch_link">
    <visual>
      <origin rpy="0 1.5707 1.5707" xyz="0 0 0"/>
      <geometry>
        <sphere radius="0.025"/>
      </geometry>
      <material name="white"/>
    </visual>
    <collision>
      <origin rpy="0 1.5707 1.5707" xyz="0 0 0" />
      <geometry>
        <cylinder length="0.015" radius="0.025"/>
      </geometry>
    </collision>
  </link>

  <joint name="front_pitch_joint" type="continuous">
    <origin rpy="0 0 0" xyz="0 0 0" />
    <parent link="front_roll_link" />
    <child link="front_pitch_link" />
    <axis xyz="0 1 0" />
    <limit effort="1000.0" velocity="100.0" />
    <dynamics damping="0.0" friction="0.1"/>
  </joint>



<!-- Caster Wheel Back -->
  <link name="back_yaw_link">
    <visual>
        <origin rpy="0 1.5707 1.5707" xyz="0 0 0"/>
        <geometry>
          <cylinder length="0.015" radius="0.025"/>
        </geometry>
        <material name="blue"/>
      </visual>
      <collision>
      <origin rpy="0 1.5707 1.5707" xyz="0 0 0" />
      <geometry>
        <cylinder length="0.015" radius="0.025"/>
